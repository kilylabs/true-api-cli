#!/usr/bin/php
<?php

$paths = [
    __DIR__.'/../vendor/autoload.php', // locally
    __DIR__.'/../../../autoload.php' // dependency
];
foreach ($paths as $path) {
    if (file_exists($path)) {
        require_once $path;
        break;
    }
}

(new Kily\API\TrueAPI\Cli\Command)->runWithTry($argv);
